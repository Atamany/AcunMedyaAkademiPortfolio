
@{
    ViewBag.Title = "CreateSocialMedia";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h3>Yeni Sosyal Medya Hesabı Girişi</h3>
<form method="post">
    <label>Platform</label>
    <input type="text" class="form-control" name="Platform" />

    <label>Link</label>
    <input type="text" class="form-control" name="Link" />

    <label>İkon (URL)</label>
    <input type="text" class="form-control" name="İkonUrl" />


    <br /><br />
    <button class="btn btn-info">Kaydet</button>
</form>



